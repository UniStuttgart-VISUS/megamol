{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "vcpkg-configuration": {
    "default-registry": {
      "kind": "builtin",
      "baseline": "1de2026f28ead93ff1773e6e680387643e914ea1"
    }
  },
  "name": "megamol",
  "version-string": "1.3.0-dev",
  "dependencies": [
    {
      "name": "adios2",
      "features": [
        "zfp"
      ]
    },
    "bhtsne",
    "blend2d",
    "chemfiles",
    "cmakerc",
    "cppzmq",
    "curl",
    "cxxopts",
    "delaunator-cpp",
    "eigen3",
    "glfw3",
    "glm",
    "imath",
    {
      "name": "imgui",
      "features": [
        "docking-experimental"
      ]
    },
    "imgui-software-renderer",
    "imguizmoquat",
    "implot",
    "libigl",
    {
      "name": "libjpeg-turbo",
      "platform": "!windows"
    },
    "libpng",
    "ltla-umappp",
    "lua",
    "mmpld-io",
    "nanoflann",
    {
      "name": "ncurses",
      "platform": "!windows"
    },
    "nlohmann-json",
    "obj-io",
    "openexr",
    "qhull",
    "quickhull",
    "simultaneous-sort",
    "snappy",
    "sol2",
    "spdlog",
    "tbb",
    "tinygltf",
    "tinyobjloader",
    "tinyply",
    "zeromq",
    "zfp",
    "zlib"
  ],
  "features": {
    "cgal": {
      "description": "Use CGAL",
      "dependencies": [
        "cgal"
      ]
    },
    "cuda": {
      "description": "Use CUDA"
    },
    "cuesdk": {
      "description": "Use Corsair CUE",
      "dependencies": [
        "cuesdk"
      ]
    },
    "mpi": {
      "description": "Use MPI",
      "dependencies": [
        {
          "name": "adios2",
          "features": [
            "mpi"
          ]
        },
        {
          "name": "icet",
          "features": [
            "mpi",
            "opengl"
          ]
        },
        "mpi"
      ]
    },
    "nvperf": {
      "description": "Use NVPerf Profiling"
    },
    "opengl": {
      "description": "Use OpenGL",
      "dependencies": [
        "glad",
        {
          "name": "glowl",
          "features": [
            "gl-extensions",
            "glm"
          ]
        },
        {
          "name": "imgui",
          "features": [
            "opengl3-binding"
          ]
        },
        {
          "name": "imgui-tex-inspect",
          "features": [
            "opengl3-binding"
          ]
        },
        "megamol-shader-factory"
      ]
    },
    "opengl-debuggroups": {
      "description": "Automatically inject OpenGL debug groups into MegaMol Call callback invocations"
    },
    "ospray": {
      "description": "Use OSPRay",
      "dependencies": [
        {
          "name": "ospray",
          "features": [
            "avx",
            "avx2"
          ]
        }
      ]
    },
    "power": {
      "description": "Use power consumption measurement",
      "dependencies": [
        {
          "name": "arrow",
          "features": [
            "parquet"
          ]
        },
        "power-overwhelming"
      ]
    },
    "power-visa": {
      "description": "Use power measurement visa sensors",
      "dependencies": [
        {
          "name": "power-overwhelming",
          "features": [
            "visa"
          ]
        }
      ]
    },
    "profiling": {
      "description": "Use Profiling"
    },
    "stacktrace": {
      "description": "Use Boost.Stacktrace",
      "dependencies": [
        "boost-stacktrace",
        {
          "name": "libbacktrace",
          "platform": "!windows"
        }
      ]
    },
    "tracy": {
      "description": "Use Tracy",
      "dependencies": [
        {
          "name": "tracy",
          "features": [
            "cli-tools",
            "gui-tools"
          ]
        }
      ]
    },
    "vr-interop": {
      "description": "Use VR Interop",
      "dependencies": [
        "mwk-mint-interop"
      ]
    },
    "vtkm": {
      "description": "Use VTK-m",
      "dependencies": [
        "vtk-m"
      ]
    }
  }
}

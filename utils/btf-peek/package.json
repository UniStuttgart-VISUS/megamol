{
    "name": "btf-peek",
    "displayName": "btf-peek",
    "description": "allows to peek into referenced snippets in btf shader index files",
    "version": "0.0.1",
    "publisher": "VISUS",
    "license": "BSD-3-Clause",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": []
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",

        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.10.5",
        "tslint": "^5.14.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.30"
    }

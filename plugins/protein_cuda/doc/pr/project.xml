        <call class="CallRender3D" from="view3d::rendering" to="sphereren::rendering" />
        <module class="CoarseGrainDataLoader" name="cgdata" />
        <call class="SphereDataCall" from="sphereren::getdata" to="cgdata::dataout" />
    </view>


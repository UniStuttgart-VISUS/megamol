#!/bin/bash
# filename: do_script_for_all_pdb_in_cache.sh

echo ====================================================================
echo Executing \"UncertaintyInputData.py\" for all PDB-files in \"../cache\":

for file in $( find ../cache/*.pdb ); do
    id=${file:9:4}
    echo ====================================================================
    echo Processing PDB-ID: $id
    python3.5 UncertaintyInputData.py -o $id | tee do_script_for_all_pdb_in_cache.log
done

